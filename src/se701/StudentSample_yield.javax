package se701;

public class StudentSample {

	public static void main(String[] args){ 
		sampleMethod(){ 
			//The yield block 
			for(int i = 0; i < 20; i++) { 
				System.out.println(i);
			}
		}; 
	} 

 	public static void sampleMethod() {
		System.out.println("Execution started");
		yield;
		System.out.println("Execution ended"); 
	}

}
